{
  "name": "@kunalnagarco/react-component-library",
  "version": "3.0.44",
  "description": "A collection of reusable React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalnagarco/react-component-library.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "find dist -type d -name '__stories__' -exec rm -r {} + && find dist -type f -name '*.stories.*' -delete",
    "build:storybook": "build-storybook -o release",
    "lint": "eslint --fix src/ --ext .ts,.tsx --max-warnings 0",
    "start": "start-storybook -p 9009",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix src/ --ext .ts,.tsx --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "highlight.js": "10.7.2",
    "immer": "8.0.4",
    "marked": "0.8.2",
    "mem": ">=4.0.0",
    "node-fetch": "2.6.1",
    "node-notifier": "8.0.2",
    "prismjs": "1.23.0",
    "yargs-parser": ">=18.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@brainhubeu/react-carousel": "1.19.26",
    "@kunalnagarco/eslint-config": "^0.1.6",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@rollup/plugin-url": "4.0.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.9",
    "@svgr/rollup": "5.5.0",
    "@types/brainhubeu__react-carousel": "1.15.0",
    "@types/node": "13.13.52",
    "@types/react": "16.14.6",
    "@types/react-dom": "16.9.13",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/parser": "2.34.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "babel-jest": "25.5.1",
    "babel-loader": "8.2.2",
    "babel-preset-react": "6.24.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "husky": "4.3.8",
    "jest": "25.5.4",
    "lint-staged": "10.5.4",
    "normalize.css": "8.0.1",
    "polished": "3.7.1",
    "postcss-import": "12.0.1",
    "prettier": "^2.3.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-scripts": "3.4.4",
    "rollup": "1.32.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-typescript2": "0.30.0",
    "semantic-release": "17.4.3",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.0",
    "styled-components": "5.3.0",
    "ttypescript": "1.5.12",
    "typescript": "^4.2.4",
    "yaml-validator": "3.0.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "publishConfig": {
    "access": "public"
  }
}
