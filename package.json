{
  "name": "@kunalnagarco/react-component-library",
  "version": "0.0.0-development",
  "description": "A collection of reusable React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalnagarco/react-component-library.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "find dist -type d -name '__stories__' -exec rm -r {} + && find dist -type f -name '*.stories.*' -delete",
    "build:storybook": "build-storybook -o release",
    "deploy": "sh ./scripts/deploy.sh && yarn build && yarn publish",
    "lint": "eslint --fix src/ --ext .ts,.tsx --max-warnings 0",
    "release": "standard-version",
    "start": "start-storybook -p 9009",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix src/ --ext .ts,.tsx --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "highlight.js": "10.4.1",
    "immer": "8.0.1",
    "marked": "0.7.0",
    "node-fetch": "2.6.1",
    "node-notifier": "8.0.1",
    "prismjs": "1.23.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@brainhubeu/react-carousel": "^1.13.20",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-url": "^4.0.2",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@svgr/rollup": "^5.3.0",
    "@types/brainhubeu__react-carousel": "^1.10.2",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "eslint-config-kunalnagar": "0.0.9",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.2",
    "jest": "^25.1.0",
    "lint-staged": "^10.2.13",
    "normalize.css": "^8.0.1",
    "polished": "^3.5.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-typescript2": "^0.27.0",
    "semantic-release": "^17.4.2",
    "sort-package-json": "^1.44.0",
    "standard-version": "^9.0.0",
    "styled-components": "^5.1.1",
    "ttypescript": "^1.5.11",
    "typescript": "~3.7.5",
    "yaml-validator": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "publishConfig": {
    "access": "public"
  }
}
